// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  age       Int
}

model Test {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}

model Patient{
  id Int @id @default(autoincrement())
  name String
  tutorName String
  species String
  age Int
  consultations Consultation[]
}

model Consultation{
  id Int @id @default(autoincrement())
  type String
  doctorName String
  date String
  time String
  description String

  idPatient Int

  patient Patient @relation(fields: [idPatient], references: [id])
}
