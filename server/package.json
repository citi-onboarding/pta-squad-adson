{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"ts-node-dev --poll --exit-child -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts\" \"prisma generate --watch\"",
    "build": "swc src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "postinstall": "prisma generate",
    "migration:deploy": "prisma migrate deploy",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "18.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "5.2.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.107",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}